            });
        });
function BindEvent(){
    $("#_main #del").click(function(){
            $(this).parents(".repeat").remove(); 
            });
    $(function(){
            $("#_main span").one("click",function(){
                var $input=$('<input align="center" type="text" name="text" class="input4" />');
         //       $input.attr("value",$(this).val());
                $(this).html("");
                $(this).append($input);
                $(".input4")[0].focus(function(){
            $(this).val("");
      //  }
    });
    $(".input4").blur(function(){
        var $inputb=$('<span align="center"  display:block;line-height:100%; class="STYLE2 STYLE1"></span>');
        $(this).parent().parent().append($inputb);
        if($(this).val()=="")
            $inputb.html(" 点此修改");            
        else
            $inputb.html($(this).val());           
        $(this).remove();
            BindEvent();
    });
   });
});

}

</script>


